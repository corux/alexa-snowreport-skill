{
  "name": "alexa-snowreport-skill",
  "version": "0.0.0",
  "description": "An Alexa skill for the Amazon Echo.",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "build": "npm-run-all --parallel build:skill",
    "build:skill": "webpack --config config/webpack.config.babel.js",
    "pretest": "npm run build",
    "test": "ava",
    "posttest": "npm run lint",
    "deploy:prod": "serverless deploy --stage production",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "ava": {
    "babel": "inherit",
    "failWithoutAssertions": false
  },
  "devDependencies": {
    "alexa-annotations": "9.2.0",
    "alexa-request": "0.3.4",
    "alexa-response": "4.2.10",
    "alexa-ssml-jsx": "2.0.3",
    "alexa-utterances": "0.2.1",
    "ava": "0.25.0",
    "babel-cli": "6.24.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-1": "6.24.1",
    "babili-webpack-plugin": "0.1.2",
    "chai": "4.2.0",
    "chai-subset": "1.6.0",
    "cheerio": "1.0.0-rc.2",
    "eslint": "^4.13.1",
    "node-aws-lambda": "0.1.8",
    "nomnom": "1.8.1",
    "npm-run-all": "4.1.3",
    "regenerator": "0.13.3",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "serverless": "1.33.2",
    "source-map-support": "0.5.9",
    "webpack": "3.12.0",
    "webpack-sources": "1.0.1"
  }
}
